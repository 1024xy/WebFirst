<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="wwwroot\vendors\echarts\**" />
    <Compile Remove="wwwroot\vendors\Flot\**" />
    <Compile Remove="wwwroot\vendors\parsleyjs\**" />
