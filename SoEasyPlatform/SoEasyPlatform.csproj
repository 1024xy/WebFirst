    <EmbeddedResource Remove="wwwroot\vendors\Flot\**" />
    <EmbeddedResource Remove="wwwroot\vendors\parsleyjs\**" />
    <None Remove="wwwroot\vendors\echarts\**" />
    <None Remove="wwwroot\vendors\Flot\**" />
    <None Remove="wwwroot\vendors\parsleyjs\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="wwwroot\template\Entity01.txt" />
    <None Remove="wwwroot\template\NET4.txt" />
    <None Remove="wwwroot\template\NET45.txt" />
    <None Remove="wwwroot\template\NET5.txt" />
    <None Remove="wwwroot\template\NETCORE31.txt" />
    <None Remove="WebFirst.exe" />
