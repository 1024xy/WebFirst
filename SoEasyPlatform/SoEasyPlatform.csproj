  <ItemGroup>
    <Resource Include="wwwroot\template\Entity01.txt" />
    <Content Include="WebFirst.exe">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <None Include="wwwroot\vendors\bootstrap\.github\CODEOWNERS" />
